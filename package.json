{
  "name": "sewing-svg-to-pdf",
  "version": "1.0.0",
  "description": "Convert Blender Seams to Sewing Pattern SVG files to printable PDFs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "npm run build && python -m http.server 8000 --directory dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "keywords": [
    "sewing",
    "pattern",
    "pdf",
    "svg",
    "blender",
    "conversion"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "clipper-lib": "^6.4.2",
    "jspdf": "^3.0.1",
    "svg2pdf.js": "^2.5.0"
  }
}
